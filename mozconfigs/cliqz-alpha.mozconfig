# Build Firefox for Android:
ac_add_options --enable-application=mobile/android
ac_add_options --target=arm-linux-androideabi
# With the following Android SDK:
ac_add_options --with-android-sdk=/home/jenkins/.mozbuild/android-sdk-linux
# Enable artifact building:
ac_add_options --enable-artifact-builds
# Write build artifacts to:
mk_add_options MOZ_OBJDIR=./objdir-frontend-android/cliqz-alpha
# Branding
ac_add_options --with-branding=mobile/android/branding/cliqz-alpha
